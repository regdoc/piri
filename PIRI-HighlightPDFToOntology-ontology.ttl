at[x=79.6908,y=416.209,w=316.1182,h=11.159988]" , "java.awt.geom.Rectangle2D$Float[x=55.6929,y=416.209,w=11.6577,h=11.159988]" , "java.awt.geom.Rectangle2D$Float[x=55.6929,y=494.20898,w=340.1281,h=11.160004]" , "java.awt.geom.Rectangle2D$Float[x=79.6893,y=403.209,w=104.6467,h=11.159973]" ;
        piri:hasPDFAnnotation  piri:measure ;
